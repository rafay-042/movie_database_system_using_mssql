
@{
    Layout = null;
}
@model  string
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Login</title>
</head>
<body>
    <div>
        <form method="post" action="../Home/authenticate">
            <input placeholder="Member Id" id="memberId" name="Id" />
            <br />
            <input placeholder="Password" id="password" name="password" />
            <br />
            <input type="submit" value="Log in" />  &nbsp;&nbsp;&nbsp; <label>@Model</label>
        </form>
    </div>
</body>
</html>
